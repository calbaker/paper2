@article{clayton_pt_2009,
	title = {Pt dispersion effects during {NOx} storage and reduction on {Pt/BaO/Al2O3} catalysts},
	volume = {90},
	issn = {0926-3373},
	url = {http://www.sciencedirect.com/science/article/pii/S0926337309001696},
	doi = {16/j.apcatb.2009.04.029},
	abstract = {This study provides insight into the effect of Pt dispersion on the overall rate and product distribution during {NOx} storage and reduction. The storage and reduction performance of {Pt/BaO/A2O3} monoliths with varied Pt dispersion (3\%, 8\%, and 50\%) and fixed Pt (2.48~wt.\%) and {BaO} (13.0~wt.\%) loadings is reported. At low temperature ({\textless}200~{{\textdegree}C)}, the differences in storage and reduction activity were the largest between the three catalysts. The amount of {NOx} stored increased with increased dispersion, as did the amount of stored {NOx} that was reduced. These trends are attributed to larger Pt surface area and {Pt-BaO} interfacial perimeter, the latter of which enhances the spillover of surface species between the precious metal and storage components. At high temperature (370~{{\textdegree}C)}, the stored {NOx} was almost completely regenerated for the three catalysts. However, the regeneration of the 3\% dispersion catalyst was much slower, suggesting a rate limitation involving the reverse spillover of stored {NOx} to Pt and/or of adsorbed hydrogen from Pt to {BaO.} The results indicate that the catalyst dispersion and operating conditions may be tuned to achieve the desired ammonia selectivity. For the aerobic regeneration feed, the most (net) {NH3} was generated by the 50\% dispersion catalyst at the lowest temperature (125~{{\textdegree}C)}, by the 3\% dispersion catalyst at the highest temperature (340~{{\textdegree}C)}, and by the 8\% dispersion catalyst at the intermediate temperatures (170-290~{{\textdegree}C).} Similar trends were observed for the net production of {NH3} with an anaerobic regeneration feed. A phenomenological picture is proposed that describes the effects of Pt dispersion consistent with the established spatio-temporal behavior of the lean {NOx} trap.},
	number = {3-4},
	journal = {Applied Catalysis B: Environmental},
	author = {Clayton, Robert D. and Harold, Michael P. and Balakotaiah, Vemuri and Wan, {C.Z.}},
	month = aug,
	year = {2009},
	keywords = {Barium, Dispersion, Hydrogen, Lean {NOx} trap, {NOx}, {NOx} storage and reduction, Platinum, Selective catalytic reduction},
	pages = {662--676}
},

