@article{balakotaiah_relationship_2008,
	title = {On the relationship between Aris and Sherwood numbers and friction and effectiveness factors},
	volume = {63},
	issn = {0009-2509},
	url = {http://www.sciencedirect.com/science/article/pii/S0009250908004569},
	doi = {16/j.ces.2008.08.025},
	abstract = {We present a general solution of the diffusion-reaction problem for linear kinetics and an expression for the effectiveness factor ([eta]) in terms of the shape normalized Thiele modulus {([Phi])} for a catalyst particle of arbitrary shape and with an arbitrary activity profile. We also show that the Sherwood number {(Sh[Omega])} or the dimensionless mass transfer coefficient between the interior of a particle or region (denoted by {[Omega])} and its boundary ([not partial {differential][Omega])} is related to the effectiveness factor and Thiele modulus by {[eta]=1/(1+[Phi]2/Sh[Omega]).} Further, the coefficients in the expansion of [eta] in terms of {[Phi]} (the Aris numbers, Ari) are related to the asymptotic Sherwood number {(Sh[Omega][infinity])} obtained in the limit of slow reaction. We show that the curve {Sh[Omega]} versus {[Phi]} is universal for most common particle or channel geometric shapes and for the case of uniform activity is described by the two asymptotes {Sh[Omega]=Sh[Omega][infinity]=1/Ar1} for {[Phi][double} less-than sign]1 and {Sh[Omega][approximate][Phi]} for {[Phi][not} double greater-than sign]1. For two-dimensional ducts we show that the friction factor times Reynolds number {(fRe)} is equal to {8Sh[Omega][infinity]} and provide a physical interpretation of this result. In the second part of this work, we derive low-dimensional models for solving multicomponent nonlinear diffusion-reaction problems using the concept of an internal mass transfer coefficient. We also present low-dimensional models for catalytic reactors using external and internal mass transfer coefficients. Finally, the {Aris/Sherwood} numbers are presented for some commonly used catalyst particles in packed-bed reactors and washcoat shapes in catalytic monoliths.},
	number = {24},
	journal = {Chemical Engineering Science},
	author = {Balakotaiah, Vemuri},
	month = dec,
	year = {2008},
	keywords = {Chemical reaction engineering, Diffusion, Mass transfer, Model reduction, Porous media, Transport processes},
	pages = {5802--5812}
},

