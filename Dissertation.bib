	title = {Inorganic Semiconductor Nanowires: Rational Growth, Assembly, and Novel Properties},
	volume = {8},
	issn = {1521-3765},
	shorttitle = {Inorganic Semiconductor Nanowires},
	url = {http://onlinelibrary.wiley.com/doi/10.1002/1521-3765(20020315)8:6<1260::AID-CHEM1260>3.0.CO;2-Q/abstract},
