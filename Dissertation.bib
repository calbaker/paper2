@article{bhattacharya_low-dimensional_2004,
	title = {Low-dimensional models for homogeneous stirred tank reactors},
	volume = {59},
	issn = {0009-2509},
	url = {http://www.sciencedirect.com/science/article/pii/S0009250904005226},
	doi = {16/j.ces.2004.07.068},
	abstract = {A novel approach is used to derive a low-dimensional model for a {CSTR} by spatial averaging of the three dimensional convection-diffusion reaction {(CDR)} equations. For the isothermal case, the model may be expressed in the two-mode form as where {\textless}C{\textgreater} and Cm are the spatial average and cup-mixing concentrations, while and tmix,2 are the space time and mixing times, respectively. The first mixing time captures the effect of micro as well as macromixing in the tank, while the second one accounts for the feed distribution (e.g., premixed, unmixed etc.). This two mode model reduces to the classical one-mode model of Bodenstein and Wolgast {[Z.} Phys. Chem. 61 (1908) 422] in the limit of tmix,i--{\textgreater}0. It is argued that for , the two mode model has the same accuracy as the full {CDR} equation while retaining qualitative features for any finite values of . The new model is used to illustrate the influence of feed distribution and mixing on conversion for the bimolecular reaction {A+B--{\textgreater}P} and competitive-consecutive reaction between A and B of the type {A+B} --{\textgreater} R and {B+R} --{\textgreater} S. The new model explains experimental results in the literature, including the opposing trends observed for series-parallel reactions under different operating conditions.},
	number = {22-23},
	urldate = {2011-07-29},
	journal = {Chemical Engineering Science},
	author = {Bhattacharya, Madhuchhanda and Harold, Michael P. and Balakotaiah, Vemuri},
	month = nov,
	year = {2004},
	keywords = {Low dimensional model, Mixing, Spatial averaging, Stirred tank reactor},
